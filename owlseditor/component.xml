<component>
	<dependencySets>
		<dependencySet>
			<useProjectArtifact>
				true
			</useProjectArtifact>
			<outputDirectory>.</outputDirectory>
			<excludes>
				<exclude>
					edu.stanford.smi.protege:protege
				</exclude>
				<exclude>
					edu.stanford.smi.protege:protege-owl
				</exclude>
			</excludes>
		</dependencySet>
	</dependencySets>
	<fileSets>
